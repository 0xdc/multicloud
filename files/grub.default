GRUB_CMDLINE_LINUX="console=tty0 console=ttyS0,115200n8 init=/usr/lib/systemd/systemd"
GRUB_CMDLINE_LINUX_DEFAULT="console=tty0 console=ttyS0,115200 no_timer_check init=/usr/lib/systemd/systemd"
GRUB_DISTRIBUTOR="Gentoo"
GRUB_GFXPAYLOAD_LINUX=text
GRUB_SERIAL_COMMAND="serial --speed=115200 --unit=0 --word=8 --parity=no --stop=1"
GRUB_TERMINAL="serial console"
